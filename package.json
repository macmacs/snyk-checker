  "scripts": {
    "findOutIfSynchronousIODetected": "node --trace-sync-io app.js",
    "startDev": "NODE_ENV=DEV nodemon $NODE_DEBUG_OPTION app.js",
    "startTest": "NODE_ENV=TEST nodemon $NODE_DEBUG_OPTION app.js",
    "startDocker": "docker run --rm node",
    "test": "mocha",
    "testStress": "mocha -r mocha-stress",
    "test:coverage": "NODE_ENV=DEV nyc mocha",
    "clean": "grunt clean",
    "lint": "grunt tslint",
    "start": "node built/app.js",
    "start:watch": "nodemon lib/app.js",
    "build": "npm run lint && echo Using TypeScript && tsc --version && tsc --pretty -p . && echo Copy static files && grunt copy",
    "build:docker": "npm run build && docker build -t node .",
    "watch": "npm run build -- --watch",
    "watch:test": "nodemon -w test -w src --exec npm run test",
    "buildAndDeployZip": "grunt releaseZip"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "all": true,
    "sourceMap": true,
    "instrument": true
